<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <div>
      <ul>
        <% messages.forEach(function(user){ %>
          <li><%= user.text %></li>
          <li><%= user.user %></li>
          <li><%= user.added %></li>
  
       <% }) %>
      </ul>
    </div>
  </body>
</html>
